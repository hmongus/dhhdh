{
    _sleep(50);
    cursor(30, 15); color(13); cout << R"(
                    
                              ___  ____   __    ___  __   __   ____ 
                             / __)(  _ \ / _\  / __)(  ) / _\ / ___)
                            ( (_ \ )   //    \( (__  )( /    \\___ \
                             \___/(__\_)\_/\_/ \___)(__)\_/\_/(____/

                         ____   __  ____      __  _  _   ___   __   ____ 
                        (  _ \ /  \(  _ \   _(  )/ )( \ / __) / _\ (  _ \
                         ) __/(  O ))   /  / \) \) \/ (( (_ \/    \ )   /
                        (__)   \__/(__\_)  \____/\____/ \___/\_/\_/(__\_)

