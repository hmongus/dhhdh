    for (int f = 0; f < FILAS; f++)
    {
        for (int c = 0; c < COLUMNAS; c++)
        {
            Console::SetCursorPosition(c, f);
            if (matriz[f][c] == 0) Console::ForegroundColor = ConsoleColor::Black;
            if (matriz[f][c] == 1) Console::ForegroundColor = ConsoleColor::Cyan;

            cout << (char)219;
        }
        //cout << endl;
    }
    system("pause>0");
    return 0;
}
